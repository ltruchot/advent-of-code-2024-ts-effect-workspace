{
  "name": "aoc-2024-workspace",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.10.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "pnpm --filter @aoc-2024-workspace/main test"
  },
  "devDependencies": {
    "@effect/eslint-plugin": "^0.2.0",
    "@eslint/js": "^9.15.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "5.3.3",
    "typescript-eslint": "^8.14.0"
  },
  "prettier": {
    "printWidth": 140,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "endOfLine": "crlf",
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  }
}
